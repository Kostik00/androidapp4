<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="4dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/coordinatorLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/h_Cod_Pasp_Place"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ДСЕ"

                    app:layout_constraintBottom_toBottomOf="@+id/Cod_Predm"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/Cod_Predm"
                    style="@style/DSETitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginEnd="4dp"
                    android:text="TextView"
                    app:layout_constraintEnd_toStartOf="@+id/imageButton"
                    app:layout_constraintStart_toEndOf="@+id/h_Cod_Pasp_Place"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/Name_Predm"
                    style="@style/DSEDescription"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="4dp"

                    android:text="TextView"
                    app:layout_constraintEnd_toStartOf="@+id/imageButton"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/Cod_Predm" />

                <TextView
                    android:id="@+id/textView4"
                    style="@style/DataCaption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:text="Паспорт"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/Name_Predm" />

                <TextView
                    android:id="@+id/Key_Pasport"
                    style="@style/DataText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="TextView"
                    app:layout_constraintBottom_toBottomOf="@+id/Cod_Pasp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/Cod_Pasp" />

                <TextView
                    android:id="@+id/textView5"
                    style="@style/DataCaption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:text="#"
                    app:layout_constraintBottom_toBottomOf="@+id/Key_Pasport"
                    app:layout_constraintEnd_toStartOf="@+id/Key_Pasport"
                    app:layout_constraintTop_toTopOf="@+id/Key_Pasport" />

                <TextView
                    android:id="@+id/Cod_Pasp"
                    style="@style/PaspTitle"
                    android:layout_width="0dp"

                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="4dp"
                    android:text="TextView"
                    app:layout_constraintBottom_toBottomOf="@+id/textView4"
                    app:layout_constraintEnd_toStartOf="@+id/textView5"
                    app:layout_constraintStart_toEndOf="@+id/textView4"
                    app:layout_constraintTop_toBottomOf="@+id/Name_Predm" />

                <ImageButton
                    android:id="@+id/imageButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@android:drawable/arrow_down_float"
                    android:contentDescription="image button1" />

                <TextView
                    android:id="@+id/textView2"
                    style="@style/DataCaption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Находится"
                    app:layout_constraintBottom_toBottomOf="@+id/Cod_Pasp_Place"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/Cod_Pasp_Place" />

                <ImageButton
                    android:id="@+id/imageButton2"
                    style="@style/DataCaption"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/Key_Pasport"
                    app:srcCompat="@android:drawable/arrow_down_float"
                    android:contentDescription="ImageButton2" />

                <TextView
                    android:id="@+id/Cod_Pasp_Place"
                    style="@style/DataText"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="2dp"
                    android:text="TextView"
                    app:layout_constraintEnd_toStartOf="@+id/PaspMaster"
                    app:layout_constraintStart_toEndOf="@+id/textView2"
                    app:layout_constraintTop_toBottomOf="@+id/Cod_Pasp" />

                <TextView
                    android:id="@+id/PaspMaster"
                    style="@style/DataText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="8dp"
                    android:text="TextView"
                    app:layout_constraintEnd_toStartOf="@+id/imageButton2"
                    app:layout_constraintTop_toTopOf="@+id/Cod_Pasp_Place" />
            </androidx.constraintlayout.widget.ConstraintLayout>


        </LinearLayout>
    </androidx.cardview.widget.CardView>

</LinearLayout>